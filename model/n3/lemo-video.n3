@prefix rdf:  		<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: 		<http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  		<http://www.w3.org/2002/07/owl#> .
@prefix xsd:		<http://www.w3.org/2001/XMLSchema#>.

@prefix lemo: 		<http://lemo.mminf.univie.ac.at/annotation-core#> .
@prefix video: 		<http://lemo.mminf.univie.ac.at/annotation-video#> .


<> a owl:Ontology;
    rdfs:label "video specific annotation vocabulary"@en;
    rdfs:comment "This vocabulary represents a video specific annotation profile of the LEMO core schema."@en;
    .

video:video-fragment a rdf:Property;
	rdfs:label "video-fragment"@en;
	rdfs:comment "The representation of an video fragment"@en;
	rdfs:domain lemo:Annotation;
	rdfs:range xsd:string;
	.