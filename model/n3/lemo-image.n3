@prefix rdf:  		<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: 		<http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  		<http://www.w3.org/2002/07/owl#> .
@prefix xsd:		<http://www.w3.org/2001/XMLSchema#>.

@prefix lemo: 		<http://lemo.mminf.univie.ac.at/annotation-core#> .
@prefix image: 		<http://lemo.mminf.univie.ac.at/annotation-image#> .


<> a owl:Ontology;
    rdfs:label "image specific annotation vocabulary"@en;
    rdfs:comment "This vocabulary represents a image specific annotation profile of the LEMO core schema."@en;
    .

image:svg-fragment a rdf:Property;
	rdfs:label "svg-fragment"@en;
	rdfs:comment "The SVG representation of an image fragment"@en;
	rdfs:domain lemo:Annotation;
	rdfs:range xsd:string;
	.